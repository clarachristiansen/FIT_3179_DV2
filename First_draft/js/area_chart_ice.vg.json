{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 300,
  "height": 200,
  "data": {
    "url": "https://raw.githubusercontent.com/clarachristiansen/FIT_3179_DV2/main/First_draft/data/Area_chart_ice/data.json",
    "format": {"type": "json"}
  },
  "mark": "area",
  "encoding": {
    "x": {
      "field": "year",
      "type": "temporal",
      "axis": {"format": "%Y"}
    },
    "y": {
      "aggregate": "sum",
      "field": "count",
      "title": "Sea ice extent ()"
    },
    "color": {
      "field": "series",
      "scale": {"scheme": "blues"}
    }
  }
}




